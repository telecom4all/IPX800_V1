{
    "name": "IPX800_V1 Addon",
    "version": "1.0.5",
    "slug": "ipx800_v1",
    "description": "An addon to interact with IPX800 devices",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "services",
    "boot": "auto",
    "options": {
      "ipx800_ip": "192.168.1.121",
      "poll_interval": 2
    },
    "schema": {
      "ipx800_ip": "str",
      "poll_interval": "int"
    },
    "host_network": true,
    "url": "https://github.com/telecom4all/IPX800_V1",
    "privileged": ["SYS_ADMIN"],
    "host_dbus": true,
    "full_access": true,
    "hassio_api": true,
    "image": "telecom4all/ipx800_v1",
    "homeassistant_api": true,
    "map": ["config:rw", "ssl"]
  }
  